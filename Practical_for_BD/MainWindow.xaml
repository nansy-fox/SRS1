<Window x:Class="Practical_for_BD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practical_for_BD"
        mc:Ignorable="d"
        Title="Авторизация сотрудников сомпьютерной компании" Height="450" Width="800" Background="White">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#30d5c8" >
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5" RenderTransformOrigin="0.53,0.477"></Image>
            <Button x:Name="BtnBack" HorizontalAlignment="Right" Content="Назад" Click="BtnBack_Click"></Button>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmName_ContentRendered"></Frame>
        <StackPanel Grid.Row="1"/>
    </Grid>
</Window>